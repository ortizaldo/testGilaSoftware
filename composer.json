{
    "require": {
        "guzzlehttp/psr7": "^1",
        "laminas/laminas-diactoros": "^2.8",
        "nyholm/psr7": "^1.4",
        "nyholm/psr7-server": "^1.0",
        "php-di/php-di": "^6.3",
        "sapphirecat/slim4-http-interop-adapter": "^1.0",
        "selective/basepath": "^2.1",
        "selective/validation": "^2.0",
        "slim/psr7": "^1.5",
        "slim/slim": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
